    <div class="container shadow" id="userRatingCtn">
        <div class="row">
            <div class="col-md-12">
                <div class="centered">
                    <div class="d-flex align-items-center justify-content-center flex-column mt-2">
                        <h3 style="color: #213360;">User Rating</h3>
                        <i id="thumbEdt" class="fa-regular fa-thumbs-up mt-2" style="font-size: 10vh; color: #fbc77f;"></i>
                        <h2 class="mt-2" style="color: #213360;">Kanin.sua</h2>
                        <div class="d-flex align-items-center mt-2 userRatingHide" id="userRatingCLick">
                            <i class="fa-solid fa-star" style="font-size: 5vh;" id="star1"></i>
                            <i class="fa-solid fa-star" style="font-size: 5vh;" id="star2"></i>
